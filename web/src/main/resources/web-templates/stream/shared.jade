extends ../../layout/streamer-layout

block styles
    style.
        @font-face {
            font-family: Shentox;
            src: url("/assets/fonts/Shentox-Regular.otf") format("opentype");
        }

        .overlay {
            font-family: Shentox;
            width: 1600px;
            height: 800px;
            display: grid;
            place-items: center;
        }

        .tick {
            font-size: 2em;
        }

    link(rel="stylesheet" href="/css/5.css")
    link(rel="stylesheet" href="/css/rvb.css")

block content
    .overlay.animate__animated

block scripts
    script(src='/js/stream/5.js')
    script(src='https://unpkg.com/d3-sankey@0')
    script(src='/js/stream/sankey.js')
    script(src='/js/stream/line-chart.js')
    script(src='/js/stream/line-chart2.js')
    script(src='/js/stream/stacked-chart.js')
    script(src='/js/stream/pie-chart.js')
    script(src='/js/stream/bar-chart.js')
    script(src='/js/stream/red-vs-blue.js')

    script(src='/js/stream/shared.js')
