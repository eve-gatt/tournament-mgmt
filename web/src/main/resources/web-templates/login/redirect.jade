doctype html
html
    head
        meta(http-equiv="Refresh", content="0; URL=" + return_url)
        style.
            body {
                background-color: #080808;
                color: #e2e2e2;
                font-size: 14px;
                overflow: hidden;
                font-family: 'Poppins', sans-serif;
            }

            a {
                color: #2a9fd6;
                text-decoration: none;
                padding-bottom: 0.2em;
                border-bottom: 2px solid transparent;
            }

            a:hover {
                border-bottom: 2px solid #f99e00;
            }


    body
        p If automatic redirect fails, please click here
            a(href=return_url)= return_url
